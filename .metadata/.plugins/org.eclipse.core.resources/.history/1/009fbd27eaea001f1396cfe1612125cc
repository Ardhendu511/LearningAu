	 void amastrongofnumber()
	 {   
//		 //System.out.println("hello world enter into amastrongnumber"); 
//		 Scanner sc=new Scanner(System.in);
//		 //System.out.println("Enter your factorial of number "); 
//		 int b=sc.nextInt();   
		 int originalNumber = 123; // Change this to any number you want
	        int number = originalNumber; // Preserve original number
	        double sum = 0; 
	        int j=3;

	        while (number > 0) {
	            int digit = number % 10;  // Extract last digit
	            sum=sum+digit*3; // Cube the digit and add to sum
	            number /= 10;  // Remove last digit
	        }

	        System.out.println("Sum of cubes of digits: " + sum);
	    }
	 
