	public void factorialnumber()
	{  
		//5!=5*4*3*2*1=120
		//6!=6*5*4*3*2*1=720
		System.out.println("program started"); 
		int number=5; 
		 int facorialnumber=1;
		for(int i=number; i>=1; i--)   
		{
			facorialnumber=facorialnumber*i;  //firstloop= 0,secondloop=0
		} 
		System.out.print(facorialnumber);
	} 
