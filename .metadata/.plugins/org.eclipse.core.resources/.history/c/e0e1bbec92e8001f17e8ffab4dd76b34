package Firstclass;

import java.util.Scanner;

public class Firstclass {

	 public static void main(String[] args) { 
		 Scanner sc=new Scanner(System.in);
		 System.out.println("Enter number"); 
		 int a=sc.nextInt();
		 if(a%a==0)
		 {
			System.out.println("even");
		 } 
		 else {
			  System.out.print("odd");
		 }
		
	
	 }
} 
 
//2|5|2
// 4
// 1 
// 5|5|1
//   5
//   0



