		 double result1 = Math.pow(2, 3);  // 2^3 = 8
	        double result2 = Math.pow(5, 2);  // 5^2 = 25
	        double result3 = Math.pow(10, 0); // Any number to power 0 is 1

	        System.out.println("2^3 = " + result1);
	        System.out.println("5^2 = " + result2);
