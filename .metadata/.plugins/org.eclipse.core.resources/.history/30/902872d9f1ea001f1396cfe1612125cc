package Thirdclass;

public class ifelseelseif {

	public static void main(String[] args) {
		ifelseelseif k1=new ifelseelseif();
		k1.factorialnumber();
     

	} 
	public void factorialnumber()
	{  
		//5!=5*4*3*2*1=120
		//6!=6*5*4*3*2*1=720
		System.out.println("program started"); 
		int number=5; 
		 int facorialnumber=0;
		for(int i=1; i<=number; i++)   
		{
			facorialnumber=facorialnumber*i;  //firstloop= 0, Secondloop=0, thridloop=0,
		} 
		System.out.print(facorialnumber);
	} 
	public void amstrongnumber()
	{
		 //int number=123
	} 
	public void leap()
	{
		System.out.print("leap year");
	} 
	public void reversednumber()
	{
		System.out.print("leap year");
	}

}
