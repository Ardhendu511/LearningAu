package Basicprogramm123;

import java.util.Scanner;

public class Basicprogramm1234 {

	public static void main(String[] args) {   
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number");
		 int a=sc.nextInt(); 
		 switch (a) {
		case 1: {
			
			System.out.print("hi first number");
		}
		default:
			System.out.print("I am in exiting from loop");
		}
		 }
}
